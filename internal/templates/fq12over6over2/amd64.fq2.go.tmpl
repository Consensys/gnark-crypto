// Mul sets z to the E2-product of x,y, returns z
func (z *E2) Mul(x, y *E2) *E2 {
	if !supportAdx {
		mulGenericE2(z, x, y)
		return z
	}
	var bc E2

	bc.A0.Mul(&x.A0, &y.A0)
	bc.A1.Mul(&x.A1, &y.A1)

	innerMulAdxE2(x, y, &bc, z)
	return z
}


