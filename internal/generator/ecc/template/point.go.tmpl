	xxg.ScalarMultiplication(&xg, &xGen).Neg(&xxg)

	res.Set(&xxg).
		SubAssign(&xg).
		SubAssign(a)

	t.Set(&xg).
		SubAssign(a).
		psi(&t)

	res.AddAssign(&t)

	t.Double(a)
	t.X.MulByElement(&t.X, &thirdRootOneG1)

