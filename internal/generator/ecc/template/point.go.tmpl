	points[1].Double(&points[0]).
		AddAssign(&points[0]).
		psi(&points[1])

	points[2].psi(&points[0]).
		psi(&points[2])

	points[3].psi(a).psi(&points[3]).psi(&points[3])

	var res {{$TJacobian}}
	res.Set(&g2Infinity)
	for i := 0; i < 4; i++ {
